	ДобавитьКоманду(ПараметрыРегистрации.Команды, 
		"Открыть форму : СубПлагин_док_ОтчетПроизводстваЗаСмену",
		"Форма_СубПлагин_док_ОтчетПроизводстваЗаСмену",
