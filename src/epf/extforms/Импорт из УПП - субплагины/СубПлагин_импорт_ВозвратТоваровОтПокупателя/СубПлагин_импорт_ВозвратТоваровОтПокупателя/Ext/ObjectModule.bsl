Функция СоздатьОбновитьОбъектРасчетов(ОбъектДанных, id, ОбъектДанныхСсылка)
	// объект расчетов
	УИД = Новый УникальныйИдентификатор(id.Ref);
	ОбъектРасчетов = Справочники.ОбъектыРасчетов.ПолучитьСсылку(уид);
	Если НЕ ЗначениеЗаполнено(ОбъектРасчетов.ВерсияДанных) Тогда
		ОбъектРасчетовОбъект = Справочники.ОбъектыРасчетов.СоздатьЭлемент();

		СсылкаНового = Справочники.ОбъектыРасчетов.ПолучитьСсылку(уид);
